{
  "name": "blogilista-ui",
  "version": "1.0.1",
  "description": "Helsinki Open University excercise Blogilista Frontend",
  "browser": "build/index.html",
  "homepage": "https://ui-blogilista-dev.now.sh",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pasunmaa/full-stack-open-2018-blogilista-frontend"
  },
  "keywords": [
    "blogilista",
    "frontend",
    "ui"
  ],
  "author": "Petri Asunmaa",
  "license": "ISC",
  "now": {
    "alias": "ui-blogilista-dev",
    "env": {
      "PORT": "3003",
      "TEST_PORT": "0",
      "NODE_ENV": "development"
    },
    "files": [
      "build",
      "rules.json",
      "package.json"
    ]
  },
  "dependencies": {
    "@material-ui/core": "^1.5.1",
    "@material-ui/icons": "^1.1.1",
    "axios": "^0.17.1",
    "prop-types": "^15.6.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-scripts": "^1.1.4",
    "redux": "^4.0.0",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "serve": "^10.0.0"
  },
  "scripts": {
    "now-start": "cross-env NODE_ENV=production serve -s build",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "proxy": "http://localhost:3003",
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "cross-env": "^5.2.0",
    "enzyme": "^3.4.4",
    "enzyme-adapter-react-16": "^1.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.21.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.1",
    "jest": "^23.5.0",
    "watchman": "^1.0.0"
  }
}
